   "source": [
    "# program to identify duplicate values from list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Counter({5: 3, 1: 2, 4: 2, 8: 2, 2: 1, 6: 1})\n",
      "[1, 4, 5, 8]\n"
     ]
    }
   ],
   "source": [
    "from collections import Counter\n",
    "\n",
    "lst = [1,4,5,2,5,1,5,6,8,4,8]\n",
    "\n",
    "dup = Counter(lst)\n",
    "print(dup)\n",
    "\n",
    "new_lst = list([item for item in dup if dup[item] > 1])\n",
    "\n",
    "print(new_lst)\n"
   ]
