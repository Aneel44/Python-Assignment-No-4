   "source": [
    "def total_sum(mydict):\n",
    "\n",
    "    total_sum = 0\n",
    "\n",
    "    for i in mydict.values():\n",
    "        total_sum = total_sum + i\n",
    "\n",
    "    return total_sum\n",
    "\n",
    "mydict = {0: 34, 1: 50, 2: 90, 3: 30}\n",
    "print(\"sum of dict:\", total_sum(mydict))\n"
   ]
